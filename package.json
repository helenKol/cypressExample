{
  "name": "MCSCloud-web",
  "version": "0.0.1",
  "description": "React web-application for MCSCloud project",
  "main": "index.js",
  "repository": "git@gitlab.osinit.com:MCSTeam/MCSCloud-web.git",
  "license": "ISC",
  "engines": {
    "node": ">=6.9"
  },
  "scripts": {
    "server": "nodemon --watch server --exec babel-node --max-old-space-size=8192 --inspect server/index.js",
    "devserver": "webpack-dev-server --config webpack.devserver.js",
    "build": "export NODE_ENV=production && rimraf dist/* && webpack --config webpack.prod.js && babel server --out-dir dist",
    "prod": "export NODE_ENV=production && node --max-old-space-size=8192 dist/index.js",
    "start": "npm run build && npm run prod",
    "cy:run": "rimraf mochawesome-report/* && cypress run",
    "cy:run:update": "yarn cy:run -- --env updateSnapshots=true",
    "cy:open": "cypress open",
    "cy:open:update": "yarn cy:open -- --env updateSnapshots=true",
    "lint": "eslint src",
    "format": "prettier-eslint --write \"src/**/*.js\"",
    "validate": "npm run lint",
    "test:merge": " mochawesome-merge â€” reportDir mochawesome-report > mochawesome-report/output.json",
    "test:build": "npm run test:merge && marge mochawesome-report/output.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate"
    }
  },
